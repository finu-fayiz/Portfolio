{
    "version": 2,
    "builds": [
        {
            "src": "my_portfolio/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "20mb", "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "my_portfolio/wsgi.py"
        }
    ]
}
